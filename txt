      UART_getc_nonblocking(UART_CAMERA,&data2[w]);
      if(data2[w] == LK_PICTURE_RE[index4]){
        index4++;
      }else{
        index4 = 0;
      }
      w++;
    }
    if(index4 == sizeof(LK_PICTURE_RE)){UART_puts(UART_GNSS, "yay64\r\n");}
